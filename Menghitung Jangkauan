
package gasken.oi;
import java.util.*;

public class GaskenOi {
       
    public static int menghitungJangkauan(int a[]){
        int jangkauan =a[a.length-1]-a[0];
        return jangkauan; 
        }
    
    public static void main (String[] args){
        Scanner s = new Scanner(System.in);
        int [] a={1,3,5,6,7,8,9,12};
        a[0]=s.nextInt();
        a[a.length-1]=s.nextInt();
        System.out.println(menghitungJangkauan(a));
    
    }
    }
